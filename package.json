{
  "name": "priorityqueue",
  "version": "1.0.0-rc1",
  "description": "An implementation of Priority Queue",
  "main": "lib/cjs",
  "module": "lib/esm",
  "sideEffect": false,
  "scripts": {
    "build:cjs": "cross-env NODE_ENV=commonjs babel src --out-dir lib/cjs --ignore *.test.js,testing/**,performance/**",
    "build:esm": "cross-env NODE_ENV=esmodule babel src --out-dir lib/esm --ignore *.test.js,testing/**,performance/**",
    "build": "run-p build:*",
    "perf:build": "cross-env NODE_ENV=commonjs babel src --out-dir lib/cjs",
    "perf:exec": "node lib/cjs/_internal/performance",
    "perf": "run-s perf:build perf:exec",
    "watch": "run-p \"build:* -- --watch\"",
    "lint": "eslint \"src/**\"",
    "typecheck": "flow",
    "prepublish": "run-s clean build",
    "clean": "rm -rf lib/*",
    "test": "cross-env NODE_ENV=test jest",
    "changelog": "conventional-changelog --preset angular --infile CHANGELOG.md --same-file --release-count 1 --append",
    "preversion": "run-s lint test typecheck build",
    "version": "run-s changelog && git add CHANGELOG.md",
    "precommit": "lint-staged",
    "ci": "run-s lint typecheck test",
    "postci": "codecov",
    "documentation": "documentation build src -f md > API.md"
  },
  "author": "berlysia",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git@github.com:berlysia/priorityqueuejs.git"
  },
  "keywords": [
    "priority",
    "queue",
    "priority queue",
    "heap",
    "data structure"
  ],
  "devDependencies": {
    "@babel/cli": "^7.5.0",
    "@babel/core": "^7.5.4",
    "@babel/plugin-proposal-class-properties": "^7.5.0",
    "@babel/plugin-proposal-export-default-from": "^7.5.2",
    "@babel/preset-env": "^7.5.4",
    "@babel/preset-flow": "^7.0.0",
    "@berlysia/eslint-config": "^0.1.0",
    "codecov": "^3.0.0",
    "conventional-changelog-cli": "^1.3.14",
    "cross-env": "^5.1.3",
    "documentation": "^5.4.0",
    "eslint": "^4.18.1",
    "eslint-plugin-eslint-comments": "^2.0.2",
    "eslint-plugin-jest": "^21.12.2",
    "flow-bin": "^0.66.0",
    "flow-typed": "^2.3.0",
    "husky": "^0.14.3",
    "jest": "^22.4.4",
    "lint-staged": "^7.0.0",
    "lodash.range": "^3.2.0",
    "npm-run-all": "^4.1.2",
    "regenerator-runtime": "^0.11.1",
    "seed-random": "^2.2.0",
    "shuffle-array": "^1.0.1",
    "simple-statistics": "^5.2.1"
  },
  "lint-staged": {
    "*.js": [
      "npm run lint"
    ]
  },
  "jest": {
    "collectCoverage": true,
    "coverageDirectory": "<rootDir>/coverage",
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!**/node_modules/**",
      "!**/testing/**"
    ],
    "transform": {
      "^.+\\.js?$": "babel-jest"
    }
  },
  "files": [
    "lib"
  ]
}
